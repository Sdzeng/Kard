/* 任务列表 */

.task-section {
  width: 100%;
  /* height:650rpx;
  overflow-y: scroll;  */
}




.task_operation_btns{
  display: flex;
  flex-direction: row;
  justify-content:center;
  align-items:center;
  padding:30rpx 20rpx;
}

.task_operation_btn{

  width:120rpx;
  height:80rpx;
  line-height:80rpx;
  text-align: center;
  background-color:#f8f8f8; 
  font-weight: 200%;
  border-radius: 10rpx;
  margin: 4rpx;
   /* -webkit-transition: all 0.2s;
  transition: all 0.2s; */


}
 

 .task_operation_btn_touch{
  /* background-color:rgba(236, 236,236, 0.3);  */
  color:#fff;
  background: #a5d6a7;
} 

.task_info {
  display: flex;
  justify-content: space-between;
  width: 100%;
  border-top: 2rpx solid #eee;
  height: 120rpx;
  line-height: 120rpx;
  overflow: hidden;
  background-color:#fff;
}

.task_info:last-child{
  border-bottom: 2rpx solid #eee;
}

.task_info_touch{
    background-color:#ececec; 
}


.task_info_content {
  width: 100%;
  padding: 10rpx 20rpx;
  overflow: hidden;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(280rpx);
  transform: translateX(280rpx);
  margin-left: -280rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.task_info_content_index {
  padding: 10rpx;
  width: 13%;
  height: 80rpx;

}

.task_info_content_index_entity{

  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  text-align: center;
 
  background: #a5d6a7;
  /* #a5d6a7 */
  /* #00c000 */
  /* #ec4d00; */
  color: #fff;

}
.task_info_content_basic {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  height: 100rpx;
  width: 87%;
  overflow: hidden;
}

.task_info_content_basic_txt{
 
}
.task_info_content_basic_txt>view:first-child {
  line-height: 60rpx;
}


.task_info_content_basic_txt>view:last-child {
  line-height: 40rpx;
  font-size: 60%;
}

.task_info_content_basic_tips{
  margin: 10rpx;
  height: 80rpx;
  display: flex;
  flex-direction: row;
  justify-content:flex-end;

}


.task_info_content_basic_tips>view{
width: 80rpx;
height: 80rpx;
line-height: 80rpx;
text-align: center;
}




 

 



.task_info_btn {
  background-color: orangered;
  width: 280rpx;
  align-items: center;
  color: #fff;
  -webkit-transform: translateX(280rpx);
  transform: translateX(280rpx);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.task_info_active .task_info_content, .task_info_active .task_info_btn {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.task_info_btn_complete {
  text-align: center;
  width: 140rpx;
  background-color: #ccc;
}

.task_info_btn_delete {
  text-align: center;
  width: 140rpx;
  background-color: #f44336;
}


.view_click_class{
   background-color:rgba(0, 0,0, 0.3); 
    color:#00c000;
}