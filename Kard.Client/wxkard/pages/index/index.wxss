.calender_info_days {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  text-align: center;
  width: 100%;
  padding: 10rpx 0;

     /* -webkit-transition: all 1s;
  transition: all 1s;  */
}

.calendar_info_days_prev, .calendar_info_days_next {
  width: 50rpx;
  height: 480rpx;
  font-size: 110%;
  display: flex;
  flex-direction: column;
  justify-content: center;

}
.calendar_info_days_prev_touch,.calendar_info_days_next_touch{
  
    color:#00c000;
}

.calender_info_days_boxs {
  font-weight: 200%;
  width: 560rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
  flex-wrap: wrap;
}

.calender_info_days_boxs_empty, .calender_info_days_boxs_grid {
  width: 80rpx;
  height:80rpx;
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  justify-content: flex-start;
}

.calender_info_days_boxs_entity {
  background-color: rgba(255, 255, 255, 0.3);
  text-align: center;
  color: #fff;
  margin: 4rpx;
  width: 72rpx;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 30rpx;
  font-weight: 300%;
}

.calender_info_days_entity_choosed {
  color: #fff;
  background-color: #00c000;
}

.calender_info_days_entity_today {
  height: 66rpx;
  border-bottom: 6rpx solid #ec4d00;
}

.calender_info_choose_yearmonths_btns {
  height: 120rpx;
  line-height: 120rpx;
  border-bottom: 1rpx solid #ff7ca0;
  display: flex;
  z-index: 1000;
  flex-direction: row;
  justify-content: space-between;
}

.calender_info_choose_yearmonths_picker_view {
  flex-grow: 1;
  width: 100%;
  height: 150px;
}

.calender_info_choose_yearmonths_picker_view_cols {
  color: #ff7ca0;
  text-align: center;
  line-height: 50px;
}

/* 任务列表 */

.task-section {
  width: 100%;
  /* height:650rpx;
  overflow-y: scroll;  */
}




.task_add{
  display: flex;
  justify-content:center;
  align-items:center;
  padding: 20rpx;
}

.task_add_btn{

  width:100rpx;
  height:100rpx;
  line-height:100rpx;
  text-align: center;
     background-color: #fff; 
  border-radius: 51rpx;
  border: 2rpx solid #eee;
  font-size:150%;
   /* -webkit-transition: all 0.2s;
  transition: all 0.2s; */
  
}

 .task_add_btn_touch{
  background-color:rgba(236, 236,236, 0.3); 
  color:#fff;
  border: 2rpx solid #fff;
} 

.task_info {
  display: flex;
  justify-content: space-between;
  width: 100%;
  border-top: 2rpx solid #eee;
  height: 120rpx;
  line-height: 120rpx;
  overflow: hidden;
  background-color:#fff;
}

.task_info:last-child{
  border-bottom: 2rpx solid #eee;
}

.task_info_touch{
    background-color:#ececec; 
}


.task_info_content {
  width: 100%;
  padding: 10rpx 20rpx;
  overflow: hidden;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(280rpx);
  transform: translateX(280rpx);
  margin-left: -280rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.task_info_content_index {
  margin: 10rpx 0;
  width: 80rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-radius: 40rpx;
  text-align: center;
  margin-right: 20rpx;
  background: #00c000;
  /* #ec4d00; */
  color: #fff;
}

.task_info_content_txt {
  display: flex;
  flex-direction: column;
  height: 100rpx;
  width: 530rpx;
  margin-right: 20rpx;
  overflow: hidden;
}

.task_info_content_txt view:first-child {
  line-height: 60rpx;
}

.task_info_content_txt view:last-child {
  line-height: 40rpx;
  font-size: 60%;
}

.task_info_btn {
  background-color: orangered;
  width: 280rpx;
  align-items: center;
  color: #fff;
  -webkit-transform: translateX(280rpx);
  transform: translateX(280rpx);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
}

.task_info_active .task_info_content, .task_info_active .task_info_btn {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}

.task_info_btn_complete {
  text-align: center;
  width: 140rpx;
  background-color: #ccc;
}

.task_info_btn_delete {
  text-align: center;
  width: 140rpx;
  background-color: #f44336;
}


.view_click_class{
   background-color:rgba(0, 0,0, 0.3); 
    color:#00c000;
}