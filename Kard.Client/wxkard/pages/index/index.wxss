.calender_info_days {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  text-align: center;
  width: 100%;
  padding: 20rpx 0;

     /* -webkit-transition: all 1s;
  transition: all 1s;  */
}

.calendar_info_days_prev, .calendar_info_days_next {
  width: 50rpx;
  height: 480rpx;
  font-size: 110%;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;

}
.calendar_info_days_prev_touch,.calendar_info_days_next_touch{
  
    color:#00c000;
}

.calender_info_days_boxs {
  font-weight: 200%;
  width: 560rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: flex-start;
  flex-wrap: wrap;
}

.calender_info_days_boxs_empty,.calender_info_days_boxs_grid_date,.calender_info_days_boxs_grid_day {
  width: 80rpx;
  height:80rpx;
  display: flex;
  flex-direction: row;
  align-items: flex-start;
  justify-content: flex-start;
}

.calender_info_date_boxs_entity {
  display: flex;
  flex-direction: column;
  justify-content:center;
  align-items:center;
  background-color:#ec4d00;
  text-align: center;
  color: #fff;
  margin: 4rpx;
  width: 72rpx;
  height: 72rpx;
  font-size: 30rpx;
  font-weight: 300%;
}
 

.calender_info_days_boxs_entity {
  background-color: rgba(255, 255, 255, 0.3);
  text-align: center;
  color: #fff;
  margin: 4rpx;
  width: 72rpx;
  height: 72rpx;
  line-height: 72rpx;
  font-size: 30rpx;
  font-weight: 300%;
}

.calender_info_days_entity_choosed {
  color: #fff;
  background-color: #00c000;
}

.calender_info_days_entity_today {
  height: 66rpx;
  border-bottom: 6rpx solid #ec4d00;
}

