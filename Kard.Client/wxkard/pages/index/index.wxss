  flex-grow: 1;
  width:96%;
  height: 400rpx;

  overflow: hidden;

}

.calender_info_choose_yearmonths_picker_view_cols {
  text-align: center;
  line-height: 50px;
}



.calender_info_choose_yearmonths_btns {

 
  display: flex;
  z-index: 1000;
  flex-direction: row;
  justify-content: space-between;

}
.calender_info_choose_yearmonths_btns view{
  width: 50%;

}
.calender_info_choose_yearmonths_btns view:last-child{

    border-left: 2rpx solid #eee;

   
